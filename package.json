{
  "name": "muchi-ts",
  "version": "1.3.1",
  "description": "Annotation based test runner for JavaScript and TypeScript, written in TypeScript.",
  "main": "index.js",
  "bin": {
    "muchi": "./bin/muchi.js"
  },
  "scripts": {
    "prestart": "tsc && yarn link",
    "start": "muchi test/*.test.ts",
    "poststart": "yarn unlink",
    "format": "pretty-quick --staged",
    "compile": "tsc",
    "precommit": "lint-staged && npm run format"
  },
  "lint-staged": {},
  "devDependencies": {
    "husky": "0.14.3",
    "lint-staged": "7.3.0",
    "prettier": "1.14.2",
    "pretty-quick": "1.6.0"
  },
  "dependencies": {
    "@types/node": "10.10.2",
    "assert": "1.4.1",
    "colors": "1.3.2",
    "dada-js": "git+https://chitson1990@bitbucket.org/chitson1990/dada-js.git",
    "reflect-metadata": "0.1.12",
    "source-map-support": "0.5.9",
    "typescript": "3.1.3"
  },
  "license": "MIT",
  "author": {
    "email": "manyanda.chitimbo@gmail.com",
    "name": "Manyanda Chitimbo"
  },
  "repository": {
    "type": "git",
    "url": "https://chitson1990@bitbucket.org/chitson1990/muchi-ts.git"
  }
}
